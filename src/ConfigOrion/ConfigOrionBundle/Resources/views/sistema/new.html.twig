{% extends 'ConfigOrionBundle::layout.html.twig' %}
{% block body%}

<h1>Crear propiedad del sistema</h1>


<div class="content-hr-both show-content">
    <form action="{{ path('sistema_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
        <p class="actions-forms">
            <button type="submit" class="btn btn-success active">
                <span class="glyphicon glyphicon-ok"></span> Guardar
            </button>
        </p>
    </form>
</div>

<div class="show-actions">
    <a class="btn btn-default active" role="button" href="{{ path('sistema') }}">
        <span class="glyphicon glyphicon-th-list"></span> Lista de propiedades del sistema
    </a>
        
</div>


{% endblock%}